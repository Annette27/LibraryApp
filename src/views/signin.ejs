          <form action="./signin" method="POST">
  
            <div   class="containerSignIn" class="col-lg-4">
           
              <h2 style="text-align: center;">Sign In</h2><br>
             
              <div class="mb-3" class="col-lg-4">
                <label for="exampleInputEmail1" >Email address</label>
                <input type="email" name="email" class="form-control" id="exampleInputEmail1" placeholder="admin@gmail.com" required>
                <label for="error1" name ="error1"><%=error1%></label>
              </div>
              <div class="mb-3">
                <label for="exampleInputPassword1" >Password</label>
                <input type="password" class="form-control" id="exampleInputPassword1" placeholder="admin12345" name="password" required>
                <label for="error2" name ="error2"><%=error2%></label>
              </div>
                <center>
              <button type="submit" id="btn1" class="btn btn-primary">Submit</button>
            </center>   <br>
              </div>
          </form>
